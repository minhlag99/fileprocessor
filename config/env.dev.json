{
  "server": {
    "port": 9000,
    "uiDir": "./ui",
    "uploadsDir": "./uploads",
    "workerCount": 2,
    "enableLan": true,
    "shutdownTimeout": 10,
    "host": "localhost",
    "allowedOrigins": "*"
  },
  "storage": {
    "defaultProvider": "local",
    "local": {
      "basePath": "./uploads"
    },
    "s3": {
      "region": "",
      "bucket": "",
      "accessKey": "",
      "secretKey": "",
      "prefix": ""
    },
    "google": {
      "bucket": "",
      "credentialFile": "",
      "prefix": ""
    }
  },
  "workers": {
    "count": 2,
    "queueSize": 50,
    "maxAttempts": 3
  },
  "features": {
    "enableLAN": true,
    "enableProcessing": true,
    "enableCloudStorage": false,
    "enableProgressUpdates": true,
    "enableAuth": false,
    "enableMediaPreview": true,
    "enableAudioPreview": true,
    "enableVideoPreview": true,
    "enableImagePreview": true,
    "enableDesignPreview": true
  },
  "ssl": {
    "enable": false,
    "certFile": "",
    "keyFile": ""
  },
  "auth": {
    "googleClientID": "",
    "googleClientSecret": "",
    "oauthRedirectURL": "http://localhost:9000/auth/callback"
  },
  "logs": {
    "level": "debug",
    "format": "text",
    "output": "console"
  }
}